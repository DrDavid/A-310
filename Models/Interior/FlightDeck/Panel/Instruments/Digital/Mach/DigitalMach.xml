<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>DigitalMach.ac</path>
	
  <animation>
    <object-name>Bezel</object-name>
    <object-name>BKG</object-name>
    <object-name>Mach.001</object-name>
    <object-name>Mach.002</object-name>
    <object-name>Mach.003</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>Mach.003</object-name>
    <object-name>Mach.002</object-name>
    <object-name>Mach.001</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   1.0 </red>
      <green> 0.4 </green>
      <blue>  0.2 </blue>
    </emission>
  </animation>

  <!-- Mach -->
  <animation>
    <type>textranslate</type>
    <object-name>Mach.003</object-name>
    <property>/velocities/mach</property>
    <factor>10</factor>
    <step>0.01</step>
    <scroll>0</scroll>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>Mach.002</object-name>
    <property>/velocities/mach</property>
    <factor>1</factor>
    <step>0.1</step>
    <scroll>0</scroll>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>Mach.001</object-name>
    <property>/velocities/mach</property>
    <factor>0.1</factor>
    <step>1</step>
    <scroll>0</scroll>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>

